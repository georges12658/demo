{
  "name": "demo",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "ts-node backend/index.ts",
    "build": "next build",
    "build:frontend": "next build",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "next start",
    "start:backend": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:backend": "eslint backend/**/*.ts",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "next": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "prisma": "^5.0.0",
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "concurrently": "^7.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/express": "^4.17.0",
    "ts-node": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/prisma__client": "^5.0.0"
  }
}